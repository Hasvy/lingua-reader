@using BlazorApp.Pages.Components
@inject NavigationManager NavigationManager

@attribute [Route(Routes.Index)]

<AuthorizeView>
    <Authorized>
        @{ NavigationManager.NavigateTo("/Reading"); }
    </Authorized>
    <NotAuthorized>
        @{ NavigationManager.NavigateTo("/Login"); }
    </NotAuthorized>
</AuthorizeView>